{
    "name": "SPX",
    "version": "1.0.0",
    "description": "An easiest way to use php in electron",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-asar --icon=icons/logo.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . SPX Stock Programme XD --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icons/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SPX Stock Programme XD\"",
        "package-linux": "electron-packager . SPX Stock Programme XD --overwrite --no-asar --platform=linux --arch=x64 --icon=icons/logo.png --prune=true --out=release-builds"
    },
    "repository": "https://github.com/cosmohacker/SPX-Stock-Programme-Xd",
    "keywords": [
        "Electron",
        "php",
        "cosmohacker"
    ],
    "author": "cosmohacker",
    "license": "CC0-1.0",
    "devDependencies": {
        "babel-plugin-styled-components": "^1.13.1",
        "electron": "^13.1.6",
        "electron-packager": "^15.2.0"
    },
    "dependencies": {
        "electron-winstaller": "^5.0.0",
        "php-server-manager": "^1.2.1"
    }
}